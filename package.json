{
  "name": "electrobun-hello-world",
  "version": "1.0.0",
  "description": "A simple Electrobun app showcasing core features",
  "scripts": {
    "dev": "electrobun dev",
    "build:css": "npx tailwindcss -i ./src/mainview/index.source.css -o ./src/mainview/index.css",
    "build": "bun run build:css && electrobun build",
    "start": "bun run build && bun run dev"
  },
  "dependencies": {
    "@letladi/unzip": "^1.0.2",
    "discord-rpc": "^4.0.1",
    "electrobun": "latest",
    "react": "^19.2.4",
    "react-dom": "^19.2.4",
    "react-icons": "^5.5.0",
    "react-router-dom": "^7.13.0",
    "unzipper": "^0.12.3"
  },
  "devDependencies": {
    "@tailwindcss/cli": "^4.1.18",
    "@types/bun": "^1.3.8",
    "@types/react": "^19.2.10",
    "@types/react-dom": "^19.2.3",
    "@types/unzipper": "^0.10.11",
    "autoprefixer": "^10.4.24",
    "postcss": "^8.5.6",
    "tailwindcss": "^4.1.18"
  }
}
